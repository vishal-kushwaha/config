# Work
    # Netskope GitHub account
    Host github
        HostName github.com
        User git
        IdentityFile ~/.ssh/id_ed25519_ns-vkushwaha
        IdentitiesOnly yes

    # Jenkins Slave 1
    Host slave1
        HostName $SLAVE1
        User ubuntu
        IdentityFile ~/.ssh/vm_secrets
        IdentitiesOnly yes

    # Jenkins Slave 2
    Host slave2
        HostName $SLAVE2
        User ubuntu
        IdentityFile ~/.ssh/vm_secrets
        IdentitiesOnly yes

